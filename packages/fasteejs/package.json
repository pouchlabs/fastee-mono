{
	"name": "fasteejs",
	"version": "1.0.0",
	"description": "Next gen blazing fast web application framework",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pouchlabs/fasteejs.git"
	},
	"keywords": [
		"fasteejs",
		"api",
		"backend",
		"http",
		"web",
		"app",
		"application",
		"framework",
		"router",
		"cloudflare",
		"websocket",
		"ws",
		"rest",
		"nodejs",
		"bun",
		"workers",
		"deno",
		"edge"
	],
	"author": "pouchlabs",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"server:dev": "node --watch server.js",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"demo":"node --watch src/lib/demo.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@pouchlab/ui": "^1.1.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@tailwindcss/typography": "^0.5.14",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.10",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.41",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^2.0.0"
	},
	"import": "./dist/index.js",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"debug": "^4.3.6",
		"detect-port": "^1.6.1",
		"esm-env": "^1.0.0",
		"eventemitter3": "^5.0.1",
		"kleur": "^4.1.5",
		"sirv": "^2.0.4",
		"zod": "^3.23.8"
	},
	"bugs": {
		"url": "https://github.com/pouchlabs/fasteejs/issues"
	},
	"homepage": "https://fasteejs.top"
}
